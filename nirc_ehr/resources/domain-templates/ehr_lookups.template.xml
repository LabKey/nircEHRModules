<?xml version="1.0" encoding="UTF-8"?>
<templates xmlns="http://labkey.org/data/xml/domainTemplate"
           xmlns:dat="http://labkey.org/data/xml"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <template xsi:type="EHRLookupsTemplateType">
        <table tableName="areas" tableDbType="TABLE">
            <dat:tableTitle>Areas</dat:tableTitle>
            <dat:columns>
                <dat:column columnName="outdoor">
                    <dat:rangeURI>boolean</dat:rangeURI>
                </dat:column>
            </dat:columns>
        </table>
    </template>

    <template xsi:type="EHRLookupsTemplateType">
        <table tableName="buildings" tableDbType="TABLE">
            <dat:tableTitle>Buildings</dat:tableTitle>
            <dat:columns>
                <dat:column columnName="area">
                    <dat:rangeURI>string</dat:rangeURI>
                </dat:column>
                <dat:column columnName="outdoor">
                    <dat:rangeURI>boolean</dat:rangeURI>
                </dat:column>
            </dat:columns>
        </table>
    </template>

    <template xsi:type="EHRLookupsTemplateType">
        <table tableName="floors" tableDbType="TABLE">
            <dat:tableTitle>Floors</dat:tableTitle>
            <dat:columns>
                <dat:column columnName="outdoor">
                    <dat:rangeURI>boolean</dat:rangeURI>
                </dat:column>
            </dat:columns>
        </table>
    </template>

    <template xsi:type="EHRLookupsTemplateType">
        <table tableName="rooms" tableDbType="TABLE">
            <dat:tableTitle>Rooms</dat:tableTitle>
            <dat:columns>
                <dat:column columnName="name">
                    <dat:rangeURI>string</dat:rangeURI>
                </dat:column>
                <dat:column columnName="floor">
                    <dat:rangeURI>string</dat:rangeURI>
                </dat:column>
                <dat:column columnName="outdoor">
                    <dat:rangeURI>boolean</dat:rangeURI>
                </dat:column>
            </dat:columns>
        </table>
    </template>

    <template xsi:type="EHRLookupsTemplateType">
        <table tableName="cage" tableDbType="TABLE">
            <dat:tableTitle>Cages</dat:tableTitle>
            <dat:columns>
                <dat:column columnName="outdoor">
                    <dat:rangeURI>boolean</dat:rangeURI>
                </dat:column>
            </dat:columns>
        </table>
    </template>

</templates>