<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="vitals" tableDbType="TABLE" useColumnOrder="true">
                <columns>
                    <column columnName="Id" />
                    <column columnName="date" />
                    <column columnName="category">
                        <columnTitle>Category</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>data_category</fkTable>
                            <fkColumnName>value</fkColumnName>
                            <fkDisplayColumnName useRawValue="true"/>
                        </fk>
                    </column>
                    <column columnName="bloodPressure" />
                    <column columnName="temp" />
                    <column columnName="heartRate" />
                    <column columnName="respRate" />
                    <column columnName="pulseRate" />
                    <column columnName="ekg">
                            <columnTitle>EKG</columnTitle>
                    </column>
                    <column columnName="pulseOximetry">
                            <columnTitle>Pulse Oximetry</columnTitle>
                    </column>

                </columns>
            </table>
        </tables>
    </metadata>
</query>