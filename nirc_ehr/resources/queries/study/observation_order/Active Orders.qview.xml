<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="date"/>
        <column name="enddate"/>
        <column name="category"/>
        <column name="area"/>
        <column name="frequency"/>
        <column name="performedby"/>
        <column name="remark"/>
        <column name="history"/>
        <column name="QCState"/>
    </columns>
    <filters>
        <filter column="isActive" operator="eq" value="true"/>
    </filters>
    <sorts>
        <sort column="date"/>
    </sorts>
</customView>