<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="CageCard" tableDbType="TABLE">
                <tableTitle>Cage Card</tableTitle>
                <javaCustomizer>org.labkey.nirc_ehr.table.NIRC_EHRCustomizer</javaCustomizer>
                <columns>
                    <column columnName="cage">
                        <columnTitle>Cage</columnTitle>
                        <url>/nirc_ehr/cageDetails.view?room=${cage/room}&amp;cage=${cage}&amp;</url>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>cage</fkTable>
                            <fkColumnName>location</fkColumnName>
                            <fkDisplayColumnName>cage</fkDisplayColumnName>
                        </fk>
                    </column>
                    <column columnName="floor">
                        <columnTitle>Floor</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>floors</fkTable>
                            <fkColumnName>floor</fkColumnName>
                            <fkDisplayColumnName>name</fkDisplayColumnName>
                        </fk>
                    </column>
                    <column columnName="area">
                        <columnTitle>Area</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>areas</fkTable>
                            <fkColumnName>area</fkColumnName>
                            <fkDisplayColumnName>description</fkDisplayColumnName>
                        </fk>
                    </column>
                    <column columnName="AccountStaff">
                        <columnTitle>Account Staff</columnTitle>
                        <fk>
                            <fkDbSchema>core</fkDbSchema>
                            <fkTable>Users</fkTable>
                            <fkColumnName>UserId</fkColumnName>
                            <fkDisplayColumnName>DisplayName</fkDisplayColumnName>
                        </fk>
                    </column>
                    <column columnName="AnimalRequestedByStaff">
                        <columnTitle>Account Staff</columnTitle>
                        <fk>
                            <fkDbSchema>core</fkDbSchema>
                            <fkTable>Users</fkTable>
                            <fkColumnName>UserId</fkColumnName>
                            <fkDisplayColumnName>DisplayName</fkDisplayColumnName>
                        </fk>
                    </column>
                    <column columnName="AnimalDelivery">
                        <columnTitle>Animal Delivery</columnTitle>
                        <fk>
                            <fkDbSchema>nirc_ehr</fkDbSchema>
                            <fkTable>AnimalDelivery</fkTable>
                            <fkColumnName>AnimalDeliveryId</fkColumnName>
                            <fkDisplayColumnName>AnimalDeliveryId</fkDisplayColumnName>
                        </fk>
                    </column>
                    <column columnName="CostCenter">
                        <columnTitle>Cost Center</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>cost_center</fkTable>
                            <fkColumnName>value</fkColumnName>
                            <fkDisplayColumnName>title</fkDisplayColumnName>
                        </fk>
                    </column>
                    <column columnName="CardFormat">
                        <columnTitle>Card Format</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>card_format</fkTable>
                            <fkColumnName>value</fkColumnName>
                            <fkDisplayColumnName>title</fkDisplayColumnName>
                        </fk>
                    </column>
                    <column columnName="CensusActivityStatus">
                        <columnTitle>Census Activity Status</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>census_activity_status</fkTable>
                            <fkColumnName>value</fkColumnName>
                            <fkDisplayColumnName>title</fkDisplayColumnName>
                        </fk>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>