                        <url>/nirc_ehr/cageDetails.view?room=${SiteCage/room}&amp;cage=${SiteCage}&amp;</url>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>cage</fkTable>
