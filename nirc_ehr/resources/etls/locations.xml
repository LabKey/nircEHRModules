<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Locations</name>
    <description>Locations data</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to location types target</description>
            <source schemaName="dbo" queryName="q_locationTypes"/>
            <destination schemaName="nirc_ehr" queryName="locationTypes"
                         bulkLoad="true" targetOption="truncate">
            </destination>
        </transform>
        <transform id="step2" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to locations target</description>
            <source schemaName="dbo" queryName="q_locations"/>
            <destination schemaName="nirc_ehr" queryName="locations"
                         bulkLoad="true" batchSize="5000" targetOption="truncate">
            </destination>
        </transform>
        <transform id="step3" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to locations mapping target</description>
            <source schemaName="dbo" queryName="q_locationsMapping"/>
            <destination schemaName="nirc_ehr" queryName="locationsMapping"
                         bulkLoad="true" batchSize="5000" targetOption="truncate">
            </destination>
        </transform>
    </transforms>
</etl>