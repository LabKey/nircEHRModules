<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Projects and Assignments</name>
    <description>Queue up Project and Assignment ETLs</description>
    <transforms>
        <transform id="step 1" type="TaskrefTransformStep">
            <taskref ref="org.labkey.di.steps.QueueJobTask">
                <settings>
                    <setting name="transformId" value="{NIRC_EHR}/project"/>
                </settings>
            </taskref>
        </transform>
        <transform id="step 2" type="TaskrefTransformStep">
            <taskref ref="org.labkey.di.steps.QueueJobTask">
                <settings>
                    <setting name="transformId" value="{NIRC_EHR}/assignment"/>
                </settings>
            </taskref>
        </transform>
    </transforms>
    <schedule>
        <cron expression="0 0 20 * * ?" /> <!--Run at 8PM every day-->
    </schedule>
</etl>